{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "bun": "1.3.7"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production",
        "changesNotSentForReview": true
      }
    }
  }
}
