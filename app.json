{
  "expo": {
    "newArchEnabled": true,
    "name": "D-Log",
    "slug": "dlog",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#dbd7d2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abhishandy.dlog"
    },
    "android": {
      "package": "com.abhishandy.dlog",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#dbd7d2"
      }
    },
    "extra": {
      "eas": {
        "projectId": "1346591a-117f-4251-b774-0a0e780d43f8"
      }
    },
    "owner": "abhishandy",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ],
      "expo-sqlite"
    ]
  }
}
